    // 文件注释的组成及其排序
    "doxdocgen.file.fileOrder": [
    
        // "file",      // @file
        // "brief", // @brief 简介
        // "author",    // 作者
        // "version",   // 版本
        // "date",      // 日期
        // "empty", // 空行
        // "copyright",// 版权
        // "empty",
        "custom"    // 自定义
    ],
    "doxdocgen.generic.customTags":[
        "@retval{indent:16}",
    ],
    "doxdocgen.generic.order": [
        "brief",
        "param",
        "return",
        "custom",
    ],
    "doxdocgen.generic.paramTemplate": "@param[in/out/in,out]{indent:17}{param}:",
    "doxdocgen.generic.briefTemplate": "@brief{indent:17}",
    "doxdocgen.generic.returnTemplate": "@return{indent:17}{type}"
